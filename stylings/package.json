{
  "name": "stylings",
  "url": "https://stylings.dev",
  "version": "1.0.2",
  "type": "module",
  "author": {
    "name": "Adam Pietrasiak"
  },
  "description": "An opinionated and joyful library for writing semantic, composable, and reusable styles in React using styled-components. Features chainable style composers, inline components, theming system, and automated style variants.",
  "keywords": [
    "react",
    "styled-components",
    "css-in-js",
    "styling",
    "theme",
    "semantic-styling",
    "composable-styles",
    "inline-components",
    "style-composers",
    "flexbox",
    "grid",
    "animations",
    "colors",
    "typography",
    "design-system"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite --config vite.config.demo.ts",
    "build": "vite --config vite.config.build.ts build",
    "preview": "vite preview",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/styled-components": "*",
    "prettier": "^3.0.3",
    "react": "*",
    "react-dom": "*",
    "styled-components": "*",
    "typescript": "*",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.9"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "color": "^5.0.0",
    "csstype": "^3.1.3",
    "fast-equals": "^5.2.2"
  },
  "peerDependencies": {
    "react": "*",
    "styled-components": "*"
  },
  "packageManager": "yarn@4.4.1"
}
